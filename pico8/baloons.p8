pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function _init()
  cartdata("adz_baloons_1")
  music(0)
  init_stars()
  init_rain()
  restart()
  level_patterns={‚ñà}

  decorations = {
    draw_moon,
  }
  decoration = function() end
end

function _draw()
  _drw()
end

function _update()
  _upd()
end

function restart()
  music(1,3000)
  rx=64
  ry=110
  r_vy=-0.5
  start_ry=ry
  bx=0
  by=0
  baloons={}
  camx=0
  camy=0
  level_pattern=‚ñà
  for i=1,2 do
   add_baloon()
  end
  n=0
  platform={
    x=20,
    sx=20,
    y=20,
    w=50,
    h=20,
    color=10,
    pattern=‚ñà,
  }
  high_score=get_high_score()
  score=0
  next_level()
  _upd = update_game
  _drw = draw_game
end

function get_score()
  return flr(start_ry-ry)
end
 
function add_baloon()
    local b= {
      x=rx+(15-rnd(30)),
      y=ry-30-rnd(10),
      sprite=192+rnd(7),
      vx=(1-rnd(2))/10,
      vy=0,
    --  vy=(1-rnd(2))/10,
    }
    b.x0=b.x-rx
    b.y0=b.y-ry
    add(baloons, b)
end
 
function lerp(a,b,t)
 return a + (b-a)*t
end
 
function update_game()
  if btn(‚¨ÖÔ∏è) then rx-=2 end
  if btn(‚û°Ô∏è) then rx+=2 end
  if btn(‚¨ÜÔ∏è) or 
     btn(‚ùé) or
     btn(üÖæÔ∏è) then
   ry-=3
  end
  bx+= (rnd(2)-1)/5
  by+= (rnd(2)-1)/5

 
 for b in all(baloons) do
  -- rabbit left => baloon right
  b.x=lerp(b.x, b.x0+rx, 0.2)
  b.y=lerp(b.y, b.y0+ry, 0.2)
  b.x0+=b.vx*sin(t())
  b.y0+=b.vy*sin(t())
 end
 
 rx = mid(0, rx, 120)
 ry += r_vy

 if ry<camy+40 then
   camy = lerp(camy, ry-148, 1.0)
   -- todo: in other places
   next_level()
 end
 
 -- collision
 for b in all(baloons) do
   if b.x+8 > platform.x and 
      b.x < platform.x+platform.w and
      b.y+8 > platform.y and
      b.y < platform.y+platform.h then
    pop_baloon(b)
   end
 end
 
 score = max(score,get_score())
  
 -- win by score
 if score >= 10000 then
   gameover()
 end
 
 -- lose: by collisoin
 if rx+8 > platform.x and 
    rx < platform.x+platform.w and
    ry+8 > platform.y and
    ry < platform.y+platform.h then
   gameover()
 end
 
 if score >= 3500 then
   platform.x = 10*cos(t()/4)+platform.sx
 end

 if score >= 4000 then
   platform.y = 10*cos(t()/8)+platform.sy
 end
 
 -- lose: no baloons
 if #baloons == 0 then
   gameover()
 end


 -- todo: only update active
 update_rain()
 update_stars()
end

function pop_baloon(b)
 r_vy += 0.1
 ry += 20
 del(baloons, b)
 sfx(15)
end

function get_high_score()
  return dget(0)
end

function next_level()
  platform.w = 20+rnd(60)
  platform.h = 10+rnd(10)
  platform.x = -20+rnd(140)+camx
  platform.y = 20+rnd(30) +camy
  platform.sx = platform.x
  platform.sy = platform.y
  platform.color = 8+rnd(8)
  platform.border_color = rnd(8)
  
  level_pattern=rnd(level_patterns)
  
  -- todo: add not on each level
  add_baloon()
  r_vy -= 0.2
  r_vy = max(r_vy, -5)
  
  decoration = rnd(decorations)
  refresh_stars() 
end

function draw_game()
 cls(0)
 
 local bg = 13
 
 if score >= 2000 then
   bg=0
 elseif score >= 1000 then
   bg=1
 elseif score >= 500 then
   bg=2
 end

 rectfill(0,0,128,128,bg)

 camera(camx,camy)

 draw_decorations()
  
 -- platform
 rectfill(
   platform.x-1,
   platform.y-1,
   platform.x+platform.w+1,
   platform.y+platform.h+1,
   platform.border_color)
 fillp(0x8421)
 rectfill(
   platform.x,
   platform.y,
   platform.x+platform.w,
   platform.y+platform.h,
   platform.color)
 fillp()



 for b in all(baloons) do
   spr(b.sprite, b.x, b.y, 1,1)
   fillp(‚ñí)
   line(b.x+4,b.y+8, rx+8, ry+4,4)
   fillp()
 end

 spr(208, rx, ry)
 
 draw_scores()
end


function draw_scores()
 camera()
 fillp(level_pattern)
 fillp() 
 if score >= high_score then
   print("new high score: "..score, 0,1,0)
   print("new high score: "..score, 0,0,14)
 else
   print("score: "..score, 0,0,15)
   print("high score: "..high_score, 0,8,15)
 end
end

function draw_decorations()
 if score >= 2000 then
   draw_stars()
 elseif score >= 1000 then
   draw_moon()
 elseif score >= 500 then
   draw_rain() 
 end
 -- decoration()
end

function draw_moon()
  local tt = t()/10
  local xx = 64+128*cos(tt)
  local yy = 128+128*sin(tt)+camy 
  circfill(xx,yy,15,15)
  fillp(‚ñ•)
  circfill(xx,yy,15,10)
  fillp()
  circfill(xx+6,yy,13, 1)
end

function draw_skyscraper()
  spr(0, 5, 5+camy, 4,4)
end


function init_stars()
  stars={}
  for i=1,100 do
    add(stars,{
     x=rnd(128),
     y=rnd(128),
    })
  end
end

function draw_stars()
  for s in all(stars) do
    if score >= 3500 then
      line(s.x,s.y+flr(camy),
           s.x,s.y+flr(camy)+2,6)
    else
      pset(
        s.x, 
        s.y+flr(camy),
        7)
    end
  end
end

function refresh_stars()
  if score >= 3000 then
    return
  end
  for s in all(stars) do
      s.y = rnd(128)
  end
end

function update_stars()
  if score < 3000 then
    return
  end
  for s in all(stars) do
      if score >= 3500 then
        s.y += 2
      else
        s.y += 1
      end
      if s.y >= 128 then
        s.y = 0
      end
  end
end


-- rain
function init_rain()
  rain={}
  for i=1,100 do
    add(rain,{
     x=rnd(128),
     y=rnd(128),
     h=rnd(2)+1,
    })
  end
end

function draw_rain()
  for s in all(rain) do
    line(
      s.x, 
      s.y+camy, 
      s.x,
      s.y+camy+s.h,
      12)
  end
end

function update_rain()
  for s in all(rain) do
    s.y += 2
    if s.y >= 128 then
      s.y = 0
    end
  end
end
-->8
function print_center(text,y,color)
  print(text,64-#text*2, y, color)
end
-->8
-- game over


function gameover()
  music(-1)
  sfx(14)
  if score > high_score then
    new_highscore = true
    dset(0, score)
  else
    new_highscore = false
  end
  gameover_time = 0
  _upd = update_gameover
  _drw = draw_gameover
end

function update_gameover()
  gameover_time += 1
  -- relax for second
  if gameover_time <= 30 then
    return
  end
  if btnp(‚ùé) or 
     btnp(‚¨ÖÔ∏è) or 
     btnp(‚û°Ô∏è) then
    restart()
  end
end

function draw_gameover()
  camera()
  if gameover_time <= 30 then
    local x, y,s
    for i =0,1000 do
      x = rnd(128)
      y = rnd(128)
      s=pget(x,y)
      pset(x,y,s/2)
    end
    return
  end
  
  
  if gameover_time >= 30 then
    cls(13)
    spr(193, 36,62)
    spr(208, 32,65)
    print_center("\^igame over", 48, 0)
    if new_highscore then
      print_center("new high score: " .. score, 75, 8+6*t()%2)
    else
      print_center("score: " .. score, 75, 14)
    end
    print_center("press ‚ùé or ‚¨ÖÔ∏è or ‚û°Ô∏è", 90)
  end
end
