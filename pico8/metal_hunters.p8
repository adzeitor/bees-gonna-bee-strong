pico-8 cartridge // http://www.pico-8.com
version 41
__lua__


function _init()
  start()  
end

function start()
  tt = 0
  player = {
    -- absolute
    x = 0,
    y = 32,
    -- in tiles
    tx = 1,
    ty = 1,
    -- destination absolute
    dx = 8,
    dy = 8,
    -- destination tiles
    dtx = 1,
    dty = 1,
    speed = 1,
    state="standing",
    box={x=0, y=0, w=8, h=8},
    money=100,
  }
  detector={
    on=false,
    x=0,
    y=0,
    w=0,
    h=0,
    box={x=0, y=0, w=0,h=0}
  }
  treasures={}
  for i=0,10 do
    add(treasures, {
      x=rnd(128),
      y=64+rnd(64-8),
      sprite=49+rnd(3),
      hidden=true,
      box={x=0,y=0,w=8,h=8},
    })
  end

  ladders={}
end

function _draw()
  cls(0)
  print("\^i$"..player.money, 5,5, 8)
  if player.state == "detect" then
    print("looking",3)
    clip(
      detector.x,
      detector.y,
      detector.w,
      detector.h)
    map(0)
    for t in all(treasures) do
      spr(t.sprite, t.x, t.y)
    end
  elseif player.state == "descend" then
    -- revealed
    for t in all(treasures) do
      if not t.hidden then
        spr(t.sprite, t.x, t.y)
      end
    end  
    clip(
      player.x,
      32,
      8+4,
      player.y-32+8+8)
    map(0)
    -- hidden
    for t in all(treasures) do
      spr(t.sprite, t.x, t.y)
    end
  else
    map(0)
    for t in all(treasures) do
      if not t.hidden then
        spr(t.sprite, t.x, t.y)
      end
    end
  end

  for l in all(ladders) do
    spr(36, l.x, l.y)
  end
    
  if detector.on then
    spr(1+tt/5%3, player.x, player.y)
    spr(6+tt/10%2, player.x+8, player.y)
  else
    spr(1, player.x, player.y)  
    spr(5, player.x+8, player.y)
  end
end

function _update()
  tt+= 1

  if btn(⬅️) then move_player(-1) end
  if btn(➡️) then move_player(1)  end
  if btn(⬆️) then move_up()  end
  if btn(⬇️) then move_down()  end
  if btnp(❎) then switch_detector() end
  update_player()
  if player.x+player.box.w >= 128 then
    start()
  end
end

function lerp(a,b,t)
  return a+(b-a)*t
end

function switch_detector()
  if player.state == "detect" then
    detector.on = false
    player.state = "standing"
    return
  end
  player.state = "detect"
  player.detect_t = 0
  detector.on = true
end

function abs_box(a)
  return {
    x1=a.x+a.box.x,
    y1=a.y+a.box.y,
    x2=a.x+a.box.x+a.box.w,
    y2=a.y+a.box.y+a.box.h,
  }
end

function collision(a,b)
  local aa = abs_box(a)
  local bb = abs_box(b)
  return 
    (aa.x2>bb.x1 and aa.x2<bb.x2 and
     aa.y2>bb.y1 and aa.y2<bb.y2) 
    or
    (bb.x2>aa.x1 and bb.x2<aa.x2 and
     bb.y2>aa.y1 and bb.y2<aa.y2) 
end

function reveal_treasures()
  if not detector.on then return end

  for t in all(treasures) do
  	 if collision(t, detector) then
  	   t.hidden = false
  	 end
  end
end

function update_player()
  if player.state == "detect" then
    player.detect_t += 2
    detector.x=player.x
    detector.y=player.y
    detector.w=16
    detector.h=8+player.detect_t
    detector.box.w=detector.w
    detector.box.h=detector.h
    reveal_treasures()

    if player.detect_t+player.y >= 128 then
      player.state = "standing"
      detector.on = false
    end
  end
end

function move_player(dirx)
  if player.state == "detect" then
    return
  end
  if player.state == "descend" then 
    return
  end

  local d = dirx*player.speed
  player.x += d
end

function move_up()
  player.y-=1
  if player.y <= 32 then
    player.y = 32
    player.state = "standing"
  end
end

function move_down()
  if detector.on then
    return
  end
  if player.y >= 128-player.box.h then
    return
  end
  -- no ladder? build
  add(ladders, {
    x=player.x,
    y=player.y+8,
  })
  player.money -= 1
  
  -- has ladder? move down
  player.y+=1
  player.state = "descend"
end
